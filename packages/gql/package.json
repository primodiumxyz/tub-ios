{
  "name": "@tub/gql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "prepare": "dotenvx run -f ./../../.env --quiet -- sh -c '[ \"$NODE_ENV\" = production ] && exit 0; pnpm hasura update-cli --version v2.44.0-beta.1'",
    "dev": "./bin/start-db.sh",
    "dev:ci": "./bin/start-db-ci.sh",
    "teardown": "docker compose down --remove-orphans --volumes",
    "extract-mutations": "tsx ./bin/extractGql.ts ./src/graphql/analytics/mutations.ts",
    "extract-queries": "tsx ./bin/extractGql.ts ./src/graphql/analytics/queries.ts",
    "extract-subscriptions": "tsx ./bin/extractGql.ts ./src/graphql/analytics/subscriptions.ts",
    "generate:types": "pnpm gql-tada generate-output",
    "db:local:console": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura console'",
    "db:local:seed-apply": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura seed apply --disable-interactive --database-name default'",
    "db:remote:upgrade-v3": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura scripts update-project-v3 --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:metadata-ic": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura metadata ic list --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:metadata-reload": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura metadata reload --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:migrate-init": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate create init --from-server --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:migrate-status": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate status --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:migrate-reset": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate delete --all --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:remove-create-token-seed": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura seed create tokens_seed --from-table token --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:apply-migrations": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate apply --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "db:remote:apply-metadata": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura metadata apply --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "test": "vitest run unit",
    "benchmark": "pnpm tsx ./__test__/lib/seed.ts && vitest run benchmarks",
    "test:watch": "vitest unit",
    "test:coverage": "vitest run unit --coverage",
    "start:cache": "tsx src/cache/server.ts",
    "k6:seed": "tsx __test__/k6/scripts/seed.ts",
    "k6:start": "cd __test__/k6 && docker-compose -f docker-compose.k6.yml up -d",
    "k6:run": "k6 run __test__/k6/scripts/load-test.ts",
    "k6:stop": "cd __test__/k6 && docker-compose -f docker-compose.k6.yml down --remove-orphans --volumes",
    "k6:clean": "rm -rf __test__/k6/results/*",
    "k6:analyze": "trap 'pnpm k6:stop && pnpm k6:clean' SIGINT; pnpm k6:seed && pnpm k6:start && pnpm k6:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@urql/core": "^5.0.6",
    "fastify": "^4.21.0",
    "gql.tada": "^1.8.9",
    "graphql": "^16.10.0",
    "graphql-ws": "^5.16.0",
    "redis": "^4.7.0",
    "tsx": "^3.12.6",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.95.2",
    "@types/k6": "^0.54.2",
    "@types/node": "^22.7.0",
    "@types/ws": "^8.5.12",
    "@vitest/coverage-v8": "^0.34.0",
    "@vitest/utils": "^2.1.1",
    "hasura-cli": "2.36.1",
    "prettier": "^3.3.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "0.31.4",
    "yaml": "^2.6.1"
  }
}
