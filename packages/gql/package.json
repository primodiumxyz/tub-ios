{
  "name": "@tub/gql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "prepare": "[ \"$NODE_ENV\" = production ] && exit 0; pnpm hasura update-cli --version v2.44.0-beta.1",
    "dev": "./bin/start-db.sh",
    "dev:ci": "./bin/start-db-ci.sh",
    "teardown": "docker compose down --remove-orphans --volumes",
    "extract-mutations": "tsx ./bin/extractGql.ts ./src/graphql/analytics/mutations.ts",
    "extract-queries": "tsx ./bin/extractGql.ts ./src/graphql/analytics/queries.ts",
    "extract-subscriptions": "tsx ./bin/extractGql.ts ./src/graphql/analytics/subscriptions.ts",
    "generate:types": "pnpm hasura:generate:types && pnpm timescale:generate:types",
    "hasura:local:console": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura console'",
    "hasura:local:seed-apply": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura seed apply --disable-interactive --database-name default'",
    "hasura:generate:types": "pnpm gql-tada generate-output",
    "hasura:remote:upgrade-v3": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura scripts update-project-v3 --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "hasura:remote:migrate-init": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate create init --from-server --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "hasura:remote:migrate-status": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate status --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "hasura:remote:migrate-reset": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate delete --all --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "hasura:remote:remove-create-token-seed": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura seed create tokens_seed --from-table token --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "hasura:remote:apply-migrations": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura migrate apply --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "hasura:remote:apply-metadata": "dotenvx run -f ./../../.env --quiet -- sh -c 'hasura metadata apply --endpoint \"$HASURA_URL\" --admin-secret \"$HASURA_ADMIN_SECRET\"'",
    "timescale:generate:types": "pgtyped -c pgtyped-timescale-config.json",
    "timescale:local:migrate": "node-pg-migrate up",
    "dtimescale:local:migrate:down": "node-pg-migrate down",
    "dtimescale:local:migrate-status": "node-pg-migrate status",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@urql/core": "^5.0.6",
    "gql.tada": "^1.8.9",
    "graphql-ws": "^5.16.0",
    "pg": "^8.11.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@pgtyped/cli": "^2.3.0",
    "@pgtyped/runtime": "^2.3.0",
    "@solana/web3.js": "^1.95.2",
    "@types/node": "^22.7.0",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.12",
    "@vitest/coverage-v8": "^0.34.0",
    "@vitest/utils": "^2.1.1",
    "hasura-cli": "2.36.1",
    "node-pg-migrate": "^6.2.2",
    "prettier": "^3.3.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "0.31.4"
  }
}
