{
  "name": "@tub/indexer-db",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "bash ./scripts/start-timescale.sh",
    "db:local:migrate": "node-pg-migrate up",
    "db:local:migrate:down": "node-pg-migrate down",
    "db:local:migrate-status": "node-pg-migrate status",
    "db:remote:migrate": "node-pg-migrate up",
    "db:remote:migrate-status": "node-pg-migrate status",
    "generate:types": "pgtyped -c pgtyped-config.json"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "pg-query-parser": "^0.3.0"
  },
  "devDependencies": {
    "@pgtyped/cli": "^2.3.0",
    "@pgtyped/runtime": "^2.3.0",
    "@types/pg": "^8.10.9",
    "node-pg-migrate": "^6.2.2",
    "typescript": "^5.0.0"
  }
}
